{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": "warn",
        "indent": ["off"],
        "linebreak-style": ["off", "unix"],
        "semi": ["off", "always"],
        "no-unused-vars": [
            "off",
            {
                "vars": "local"
            }
        ],
        "no-console": ["off"],
        "no-undef": ["off"]
    },
    "globals": {
        "gracefulShutdown": "readonly",
        "_language": "writable"
    },
    "parser": "@babel/eslint-parser"
}
